{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [{
            "alignment": "left",
            "segments": [{
                    "foreground": "#F5F5F5",
                    "leading_diamond": "",
                    "properties": {
                        "prefix": "  ",
                        "alpine": "",
                        "arch": "",
                        "centos": "",
                        "debian": "",
                        "elementary": "",
                        "fedora": "",
                        "gentoo": "",
                        "linux": "",
                        "macos": "",
                        "manjaro": "",
                        "mint": "",
                        "opensuse": "",
                        "raspbian": "",
                        "ubuntu": "",
                        "windows": "",
                        "wsl": "",
                        "wsl_separator": " on "
                    },
                    "style": "powerline",
                    "type": "os"
                },
                {
                    "foreground": "#fafafa",
                    "invert_powerline": false,
                    "properties": {
                        "prefix": " ",
                        "display_host": false,
                        "display_default": false,
                        "host_color": "#d6d6d6",
                        "user_color": "#BFFF10",
                        "user_info_separator": "<#7a7a7a></>"
                    },
                    "style": "powerline",
                    "type": "session"
                },
                {
                    "foreground": "#e9e9e9",
                    "properties": {
                        "root_icon": ""
                    },
                    "style": "powerline",
                    "type": "root"
                },
                {
                    "foreground": "#ffb026",
                    "properties": {
                        "mapped_locations": {
                            "C:\\Users\\Nirmala\\Desktop": "   ",
                            "C:\\Users\\Nirmala\\Desktop\\Dev": "謁",
                            "C:\\Users\\Nirmala": " ",
                            "C:\\Windows\\System32": ""
                        },
                        "postfix": "<#FFFFFF> ~></>",
                        "prefix": "<#00B9FF> </><#e9e9e9>:\\</>",
                        "enable_hyperlink": false,
                        "folder_separator_icon": "|",
                        "folder_icon": "<#FFFF12> </>",
                        "style": "letter"
                    },
                    "style": "powerline",
                    "type": "path"
                },

                {
                    "foreground": "#EEF6FF",
                    "properties": {
                        "ahead_color": "#C2C206",
                        "behind_color": "#8A4FFF",
                        "color_background": false,
                        "display_stash_count": true,
                        "display_status": true,
                        "display_upstream_icon": true,
                        "local_changes_color": "#EEF6FF",
                        "prefix": "<#616163>  :</>",
                        "staging_color": "#8A4FFF",
                        "status_colors_enabled": true,
                        "working_color": "#ad49ff"
                    },
                    "style": "powerline",
                    "type": "git"
                }

            ],
            "type": "prompt"
        },
        {
            "alignment": "right",
            "segments": [{
                    "foreground": "#62d365",
                    "invert_powerline": true,
                    "properties": {
                        "display_version": true,
                        "postfix": "  "
                    },
                    "style": "powerline",
                    "type": "node"
                },
                {
                    "foreground": "#00acd7",
                    "invert_powerline": true,
                    "properties": {
                        "display_version": true,
                        "postfix": "  "
                    },
                    "style": "powerline",
                    "type": "go"
                },
                {
                    "foreground": "#4063D8",
                    "invert_powerline": true,
                    "properties": {
                        "display_version": true,
                        "postfix": "  "
                    },
                    "style": "powerline",
                    "type": "julia"
                },
                {
                    "foreground": "#FFEB41",
                    "invert_powerline": true,
                    "properties": {
                        "display_mode": "context",
                        "display_version": true,
                        "display_virtual_env": true,
                        "prefix": "  "
                    },
                    "style": "powerline",
                    "type": "python"
                },
                {
                    "foreground": "#AE1401",
                    "invert_powerline": true,
                    "properties": {
                        "display_mode": "files",
                        "display_version": true,
                        "postfix": "  "
                    },
                    "style": "powerline",
                    "type": "ruby"
                },
                {
                    "foreground": "#FEAC19",
                    "invert_powerline": true,
                    "properties": {
                        "display_mode": "files",
                        "display_version": false,
                        "postfix": " "
                    },
                    "style": "powerline",
                    "type": "azfunc"
                },
                {
                    "background_templates": [
                        "{{if contains \"default\" .Profile}}#FFA400{{end}}",
                        "{{if contains \"jan\" .Profile}}#f1184c{{end}}"
                    ],
                    "foreground": "#ffffff",
                    "invert_powerline": true,
                    "properties": {
                        "display_default": false,
                        "postfix": "  "
                    },
                    "style": "powerline",
                    "type": "aws"
                },

                {
                    "foreground": "#C2FF6C",
                    "invert_powerline": true,
                    "properties": {
                        "postfix": "  ",
                        "time_format": "3:04:05 PM"
                    },
                    "style": "powerline",
                    "type": "time"
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "left",
            "newline": true,
            "segments": [{
                "foreground": "#FF2740",
                "properties": {
                    "text": "#@~>"
                },
                "style": "plain",
                "type": "text"
            }],
            "type": "prompt"
        },
        {
            "alignment": "right",
            "segments": [{
                    "foreground": "#7a7a7a",
                    "properties": {
                        "text": "❮"
                    },
                    "style": "plain",
                    "type": "text"
                },
                {
                    "foreground": "#cacaca",
                    "invert_powerline": true,
                    "properties": {
                        "postfix": " 羽 "
                    },
                    "style": "powerline",
                    "type": "executiontime"
                },
                {
                    "foreground": "#cccccc",
                    "invert_powerline": true,
                    "properties": {
                        "always_enabled": true,
                        "always_numeric": true,
                        "color_background": false,
                        "display_exit_code": true,
                        "error_color": "#E32626",
                        "prefix": "  "
                    },
                    "style": "powerline",
                    "type": "exit"
                }
            ],
            "type": "rprompt"
        }
    ],
    "console_title": true,
    "console_title_style": "template",
    "console_title_template": "{{if .Root}}root :: {{end}}{{.Shell}} :: {{.Folder}}",
    "final_space": true
}